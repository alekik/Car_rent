<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.11.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="Car_rent">
        <addColumn tableName="CAR">
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATETIME"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="DATETIME"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME"/>
        </addColumn>
    </changeSet>
    <changeSet id="2" author="Car_rent">
        <addColumn tableName="CLIENT">
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATETIME"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="DATETIME"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME"/>
        </addColumn>
    </changeSet>
    <changeSet id="3" author="Car_rent">
        <addColumn tableName="DEAL">
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATETIME"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="DATETIME"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME"/>
        </addColumn>
    </changeSet>
    <changeSet id="4" author="Car_rent">
        <addColumn tableName="USER_">
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATETIME"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="DATETIME"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME"/>
        </addColumn>
    </changeSet>
    <changeSet id="5" author="Car_rent">
        <addColumn tableName="CAR">
            <column name="VERSION" type="INT"/>
        </addColumn>

        <addNotNullConstraint columnName="VERSION" defaultNullValue="0" tableName="CAR"/>
    </changeSet>
    <changeSet id="6" author="Car_rent">
        <addColumn tableName="CLIENT">
            <column name="VERSION" type="INT"/>
        </addColumn>

        <addNotNullConstraint columnName="VERSION" defaultNullValue="0" tableName="CLIENT"/>
    </changeSet>
    <changeSet id="7" author="Car_rent">
        <addColumn tableName="DEAL">
            <column name="VERSION" type="INT"/>
        </addColumn>

        <addNotNullConstraint columnName="VERSION" defaultNullValue="0" tableName="DEAL"/>
    </changeSet>
</databaseChangeLog>